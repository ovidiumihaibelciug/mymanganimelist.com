{
  "version": 2,
  "name": "mymanganimelist.com",
  "builds": [
    { "src": "next.config.js", "use": "@now/next" }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "dashboard"
    },
    {
      "src": "/anime/category/(.*)",
      "dest": "anime/category"
    },
    {
      "src": "/anime/list/(?<type>[^/]+)$",
      "dest": "anime/list?type=$type"
    },
    {
      "src": "/anime",
      "dest": "anime/list"
    },
    {
      "src": "/anime/(?<slug>[^/]+)$",
      "dest": "anime?slug=$slug"
    },
    {
      "src": "/manga/(.*)",
      "dest": "view/manga"
    },
    {
      "src": "/manga/slug/(.*)",
      "dest": "view/manga"
    },
    {
      "src": "/manga/list/(.*)",
      "dest": "list/type"
    },
    {
      "src": "/episode/(.*)",
      "dest": "episode/view"
    },
    {
      "src": "/anime/(.*)/characters",
      "dest": "characters/anime/list"
    },
    {
      "src": "/anime/(.*)/episodes",
      "dest": "episodes/list"
    },
    {
      "src": "/manga/(.*)/characters",
      "dest": "characters/manga/list"
    },
    {
      "src": "/characters",
      "dest": "characters/list"
    },
    {
      "src": "/characters",
      "dest": "characters/list"
    },
    {
      "src": "/search",
      "dest": "search"
    },
    {
      "src": "/feed",
      "dest": "discussions"
    },
    {
      "src": "/manga/(.*)/chapters",
      "dest": "chapters/list"
    },
    {
      "src": "/my-account",
      "dest": "user/my-account"
    },
    {
      "src": "/user/(.*)",
      "dest": "user/view"
    },
    {
      "src": "/login",
      "dest": "login"
    },
    {
      "src": "/post/(.*)",
      "dest": "post"
    },
    {
      "src": "/posts/(.*)",
      "dest": "post"
    },
    {
      "src": "/",
      "dest": "dashboard"
    }
  ]
}

