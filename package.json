{
  "name": "create-next-example-app",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "lint": "eslint . --ext .jsx --ext .js --fix",
    "svg-icons": "svgo --config .svg/svgo-icons.config.json -f .svg/icons&& svgr --ext jsx --icon --no-svgo --tab-width 4 -d components/svg/icons .svg/icons&& create-index ./imports/ui/components/svg/icons --extensions js jsx",
    "svg-imgs": "svgo --config .svg/svgo-imgs.config.json -f .svg/imgs&& svgr --ext jsx --no-svgo --keep-useless-defs --tab-width 4 -d components/svg/imgs .svg/imgs",
    "svg-imgs-unstyled": "svgo --config .svg/svgo-icons.config.json -f .svg/imgs-unstyled&& svgr --ext jsx --no-svgo --tab-width 4 -d components/svg/imgs .svg/imgs-unstyled",
    "lint-staged": "lint-staged",
    "start": "node server.js -p $PORT",
    "export": "npm run build && next export"
  },
  "dependencies": {
    "@svgr/core": "^4.1.0",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.13.0",
    "axios": "^0.18.0",
    "chart.js": "^2.7.3",
    "classnames": "^2.2.6",
    "easify": "^0.2.0",
    "express": "^4.16.4",
    "font-awesome": "^4.7.0",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "next": "7.0.2",
    "next-routes": "^1.4.2",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.7.0",
    "react-ga": "^2.5.7",
    "react-id-swiper": "^1.6.8",
    "react-modal-video": "^1.2.1",
    "react-molecule": "^0.1.5",
    "react-router-dom": "^4.3.1",
    "simpl-schema": "^1.5.3",
    "svgo": "^1.1.1",
    "svgr": "^1.9.2",
    "uniforms": "^1.31.0",
    "uniforms-antd": "^1.31.0"
  }
}
